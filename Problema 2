package problema_1;
import java.util.Scanner;
public class problema_2 {
    public static void main(String[] args) {
                int num = 5 ,num1 = 10;
        while (num<=30) {
            System.out.println(num + "/" + num1);
            num+=5;
            num1+=2;
        }
    }
    
    
}
